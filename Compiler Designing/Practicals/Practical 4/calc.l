%{
#include "y.tab.h"  /* Include the header generated by yacc */
%}

%%

[0-9]+  { yylval = atoi(yytext); return NUMBER; }
[\t ]+  { /* Skip whitespaces */ }
\n      { return '\n'; }
"+"     { return '+'; }
"-"     { return '-'; }
"*"     { return '*'; }
"/"     { return '/'; }
"("     { return '('; }
")"     { return ')'; }

.       { /* Catch all other invalid characters */ }

%%

int yywrap(void) {
    return 1;
}
